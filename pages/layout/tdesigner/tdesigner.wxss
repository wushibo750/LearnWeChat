/* pages/layout/tdesigner/tdesigner.wxss */
.box {
  margin: 0 32rpx;
}

.external-class {
  margin-top: 32rpx;
}

.full-width {
  background: rgba(0, 0, 0, 0.4);
  padding: 16rpx 0;
  margin-top: 32rpx;
}

.full-width .external-class {
  margin-top: 0;
}

.no-border {
  border: 0;
}

.no-border-radius {
  border-radius: 0;
}

